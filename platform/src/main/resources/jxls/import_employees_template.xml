<?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>
    <worksheet>
        <loop startRow="11" endRow="1000" items="employees" var="e" varType="ru.abondin.hreasy.platform.service.admin.employee.dto.ImportEmployeeExcelDto">
            <section startRow="11" endRow="1000">
                <mapping row="11" col="2">e.displayName</mapping>
                <mapping row="11" col="17">e.email</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0">Employee Payment Totals:</cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>